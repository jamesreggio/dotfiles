append_to_path() {
  [ -z $TMUX ] && export PATH="$PATH:$1"
}
prepend_to_path() {
  [ -z $TMUX ] && export PATH="$1:$PATH"
}

append_to_path ./node_modules/.bin
append_to_path $HOME/.fastlane/bin
append_to_path $HOME/bin

export ANDROID_SDK=$HOME/Library/Android/sdk
export ANDROID_NDK=$HOME/Library/Android/ndk-r10e
append_to_path $ANDROID_SDK/tools
append_to_path $ANDROID_SDK/platform-tools
